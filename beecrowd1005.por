programa {
  funcao inicio() {
    //declaração de variáveis
    real nota1
    real nota2
    real media
    //entrada de dados
    escreva(" nota 1: ")
    leia(nota1)

    escreva(" nota 2: ")
    leia(nota2)

    //processamento
    media = (nota1*3.5 + nota2*7.5) / 11
    
    //Saída de dados
    escreva("Média : ", media)   




  }
}
