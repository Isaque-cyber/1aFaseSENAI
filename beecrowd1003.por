programa {
  funcao inicio() {
    
      //declaração de variáveis
      inteiro numero1, numero2, soma

      //entrada de dados
      escreva("Digite o primeiro número: ")
      leia(numero1)

      escreva("Digite o segundo número: ")
      leia(numero2)

      //processamento
      soma = numero1 + numero2

      //Saída de dados
      escreva("SOMA =  " + soma)







  }
}
